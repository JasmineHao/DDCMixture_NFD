doi = {10.1016/j.jinteco.2017.05.001},
file = {:C$\backslash$:/Users/Jasmine/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bas, Mayer, Thoenig - 2017 - From micro to macro Demand, supply, and heterogeneity in the trade elasticity.pdf:pdf},
issn = {18730353},
journal = {Journal of International Economics},
keywords = {Firm-level data,Gravity,Heterogeneity,Log-normal,Pareto,Trade elasticity},
title = {{From micro to macro: Demand, supply, and heterogeneity in the trade elasticity}},
