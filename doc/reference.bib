file = {:C$\backslash$:/Users/Jasmine/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Chernozhukov et al. - 2018 - Doubledebiased machine learning for treatment and structural parameters.pdf:pdf},
issn = {1368423X},
journal = {Econometrics Journal},
number = {1},
pages = {C1--C68},
title = {{Double/debiased machine learning for treatment and structural parameters}},
volume = {21},
year = {2018}
}
@article{Chetverikov2016,
abstract = {In this paper, we derive a rate of convergence of the Lasso estimator when the penalty parameter {\$}\backslashlambda{\$} for the estimator is chosen using {\$}K{\$}-fold cross-validation; in particular, we show that in the model with the Gaussian noise and under fairly general assumptions on the candidate set of values of {\$}\backslashlambda{\$}, the prediction norm of the estimation error of the cross-validated Lasso estimator is with high probability bounded from above up to a constant by {\$}(s\backslashlog p /n){\^{}}{\{}1/2{\}}\backslashcdot \backslashlog{\^{}}{\{}7/8{\}}(p n){\$}, where {\$}n{\$} is the sample size of available data, {\$}p{\$} is the number of covariates, and {\$}s{\$} is the number of non-zero coefficients in the model. Thus, the cross-validated Lasso estimator achieves the fastest possible rate of convergence up to a small logarithmic factor {\$}\backslashlog{\^{}}{\{}7/8{\}}(p n){\$}. In addition, we derive a sparsity bound for the cross-validated Lasso estimator; in particular, we show that under the same conditions as above, the number of non-zero coefficients of the estimator is with high probability bounded from above up to a constant by {\$}s\backslashlog{\^{}}5(p n){\$}. Finally, we show that our proof technique generates non-trivial bounds on the prediction norm of the estimation error of the cross-validated Lasso estimator even if the assumption of the Gaussian noise fails; in particular, the prediction norm of the estimation error is with high-probability bounded from above up to a constant by {\$}(s\backslashlog{\^{}}2(p n)/n){\^{}}{\{}1/4{\}}{\$} under mild regularity conditions.},
archivePrefix = {arXiv},
