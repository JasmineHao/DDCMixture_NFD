journal = {Journal of Econometrics},
keywords = {Equilibrium beliefs,Game-theoretic econometric models,Semiparametric estimation,Trimming},
number = {2},
pages = {409--431},
title = {{Semiparametric estimation of a simultaneous game with incomplete information}},
url = {www.elsevier.com/locate/jeconom},
volume = {157},
year = {2010}
}
