@article{Konig2017,
abstract = {We study from both a theoretical and an empirical perspective how a network of military alliances and enmities affects the intensity of a conflict. The model combines elements from network theory and from the politico-economic theory of conflict. We obtain a closed-form characterization of the Nash equilibrium. Using the equilibrium conditions, we perform an em-pirical analysis using data on the Second Congo War, a conflict that involves many groups in a complex network of informal alliances and rivalries. The estimates of the fighting externalities are then used to infer the extent to which the conflict intensity can be reduced through (i) dismantling specific fighting groups involved in the conflict; (ii) weapon embargoes; (iii) inter-ventions aimed at pacifying animosity among groups. Finally, with the aid of a random utility model, we study how policy shocks can induce a reshaping of the network structure.},
author = {K{\"{o}}nig, Michael D and Rohner, Dominic and Thoenig, Mathias and Zilibotti, Fabrizio and Muller, Valentin and Ottinger, Sebastian and Python, Andr{\'{e}} and Schaefer, Timo and Schief, Matthias and Zorzi, Nathan and Zwyssig, Laura and Acemoglu, Daron and Adda, Jerome and Alesina, Alberto and Casella, Alessandra and {Dal Bo}, Ernesto and Dell, Melissa and Griffith, Rachel and Hemous, David and Humphreys, Macartan and Morelli, Massimo and Olken, Benjamin and {S{\'{a}}ez Mart{\'{i}}}, Mar{\'{i}}a and Sunde, Uwe and Udry, Chris and {Vega Redondo}, Fernando and Robin, Jean-Marc and Yanagizawa-Drott, David and Zanella, Giulio},
file = {:C$\backslash$:/Users/Jasmine/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/K{\"{o}}nig et al. - 2017 - Networks in Conflict Theory and Evidence from the Great War of Africa.pdf:pdf},
title = {{Networks in Conflict: Theory and Evidence from the Great War of Africa *}},
url = {http://econ.sites.olt.ubc.ca/files/2017/03/pdf{\_}paper{\_}seminar{\_}theonig.pdf},
year = {2017}
