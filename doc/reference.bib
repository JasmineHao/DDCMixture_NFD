@article{Chernozhukov2007,
abstract = {This paper develops a framework for performing estimation and inference in econometric models with partial identification, focusing particularly on models characterized by moment inequalities and equalities. Applications of this framework include the analysis of game-theoretic models, revealed preference restrictions, regressions with missing and corrupted data, auction models, structural quantile regressions, and asset pricing models. Specifically, we provide estimators and confidence regions for the set of minimizers OI of an econometric criterion function Q(O). In applications, the criterion functio embodies testable restrictions on economic models. A parameter value 0 that describe an economic model satisfies these restrictions if Q(0) attains its minimum at this value Interest therefore focuses on the set of minimizers, called the identified set. We use the inversion of the sample analog, Q,(O), of the population criterion, Q(0), to construc estimators and confidence regions for the identified set, and develop consistency, rates of convergence, and inference results for these estimators and regions. To derive thes results, we develop methods for analyzing the asymptotic properties of sample criterio functions under set identification.},
author = {Chernozhukov, Victor and Hong, Han and Tamer, Elie},
file = {:C$\backslash$:/Users/Jasmine/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Chernozhukov, Hong, Tamer - 2007 - Estimation and Confidence Regions for Parameter Sets in Econometric Models.pdf:pdf},
journal = {Econometrica},
