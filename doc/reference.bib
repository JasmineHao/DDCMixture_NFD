@techreport{Aguirregabiria2012c,
abstract = {This paper deals with the identi{\ldots}cation and estimation of dynamic games when players' beliefs about other players' actions are biased, i.e., beliefs do not represent the probability distribution of the actual behavior of other players conditional on the information available. First, we show that an exclusion restriction, typically used to identify empirical games, provides testable nonparametric restrictions of the null hypothesis of equilibrium beliefs in dynamic games with either {\ldots}nite or in{\ldots}nite horizon. We use this result to construct a simple Likelihood Ratio test of equilibrium beliefs. Second, we prove that this exclusion restriction, together with consistent estimates of beliefs at two points in the support of the variable involved in the exclusion restriction, is su¢ cient for nonparametric point-identi{\ldots}cation of players' belief functions as well as useful functions of payo¤s. Third, we propose a simple two-step estimation method. We illustrate our model and methods using both Monte Carlo experiments and an empirical application of a dynamic game of store location by retail chains. The key conditions for the identi{\ldots}cation of beliefs and payo¤s in our application are the following: (a) the previous year's network of stores of the competitor does not have a direct e¤ect on the pro{\ldots}t of a {\ldots}rm, but the {\ldots}rm's own network of stores at previous year does a¤ect its pro{\ldots}t because the existence of sunk entry costs and economies of density in these costs; and (b) {\ldots}rms'beliefs are unbiased in those markets that are close, in a geographic sense, to the opponent's network of stores, though beliefs are unrestricted, and potentially biased, for unexplored markets which are farther away from the competitors'network. Our estimates show signi{\ldots}cant evidence of biased beliefs. Furthermore, imposing the restriction of unbiased beliefs generates a substantial attenuation bias in the estimate of competition e¤ects.},
author = {Aguirregabiria, Victor and Magesan, Arvind},
file = {:C$\backslash$:/Users/Jasmine/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Aguirregabiria et al. - 2012 - Identi{\ldots}cation and Estimation of Dynamic Games when Players'Beliefs Are Not in Equilibrium.pdf:pdf},
keywords = {Dynamic games,Estimation,Identi{\ldots}cation,Market entry-exit,Rational behavior,Rationalizability},
title = {{Identification and Estimation of Dynamic Games when Players'Beliefs Are Not in Equilibrium}},
url = {http://aguirregabiria.net/wpapers/beliefs.pdf},
year = {2012}
}
