@article{Bursztyn2016,
abstract = {Students may care deeply about their image or what others think of them, and these concerns may affect schooling behavior. We identify two potentially important peer cultures: one that stigmatizes effort (thus, where it is " smart to be cool ") and one that rewards ability (where it is " cool to be smart "). We build a model that shows that either may lead to lower educational effort when effort and performance are potentially observable by peers. We design a field experiment to both test whether students are influenced by these concerns in general, and to separately test which of these concerns they are responding more to. We implement the experiment in two different settings: a high school in a low income, high minority share area and two high schools in higher income, lower minority share areas. In both settings, fewer students sign up for a lottery to get complimentary access to an online SAT prep package when they are unsure that their decision would stay completely private from their classmates. We further show that this behavior is consistent with a greater concern for hiding effort in the lower income, higher minority share school, and a greater concern with hiding low ability in the higher income, lower minority share schools.},
author = {Bursztyn, Leonardo and Egorov, Georgy and Jensen, Robert and AliBob, Ahmed and Baclini, Natalia and Burch, Cameron and De, Diego and Peza, La and Fiorin, Stefano and Galashin, Misha and Korovkin, Vasily and Mcnabb, Shelby and Miller, Matthew and Smith, Benjamin},
doi = {10.3386/w23020},
file = {:C$\backslash$:/Users/Jasmine/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bursztyn et al. - 2016 - Cool to be Smart or Smart to be Cool Understanding Peer Pressure in Education.pdf:pdf},
keywords = {education,experiment,peer pressure,signaling,stigma},
title = {{Cool to be Smart or Smart to be Cool? Understanding Peer Pressure in Education}},
url = {http://home.uchicago.edu/{~}bursztyn/Bursztyn{\_}Egorov{\_}Jensen{\_}2016{\_}07.pdf},
year = {2016}
}
